{
  "name": "joe-subgraph-v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prepare:bob_testnet": "mustache config/bob_testnet.json src/constants/index.template.ts > src/constants/index.ts && mustache config/bob_testnet.json subgraph.template.yaml > subgraph.bob_testnet.yaml",
    "codegen:bob_testnet": "graph codegen subgraph.bob_testnet.yaml",
    "build:bob_testnet": "graph build subgraph.bob_testnet.yaml",
    "deploy:bob_testnet": "graph deploy --node https://api.thegraph.com/deploy/ traderjoe-xyz/joe-v2 subgraph.avax.yaml",
    "create-local": "graph create joe-v2 --node http://localhost:8020/",
    "remove-local": "graph remove joe-v2 --node http://localhost:8020/",
    "deploy-local": "graph deploy joe-v2 ./subgraph.bob_testnet.yaml --node http://localhost:8020/ --ipfs http://localhost:5001 -l 0.0.0 ",
    "start:node": "rm -rf ./graph_data && docker compose up -d"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.80.0",
    "@graphprotocol/graph-ts": "0.35.1",
    "mustache": "^4.1.0"
  },
  "author": "traderjoexyz",
  "license": "MIT"
}